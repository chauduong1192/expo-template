{
  "name": "expo-template",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "clean": "git clean -xdf .expo node_modules",
    "start": "expo start --ios --android",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "test": "jest --watch --coverage=false --changedSince=origin/main",
    "test:coverage": "jest",
    "check-types": "tsc --noEmit",
    "lint:eslint": "eslint ./src --ext .js,.jsx,.ts,.tsx",
    "prettier": "prettier --write 'src/**/*.{js,jsx,ts,tsx,scss}'",
    "validate": "yarn run prettier & yarn run lint:eslint & yarn run check-types",
    "prepare": "husky install",
    "generate:component": "plop component",
    "generate:feature": "plop feature",
    "storybook:web": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "storybook-generate": "sb-rn-get-stories --config-path .ondevice",
    "storybook-watch": "sb-rn-watcher --config-path .ondevice",
    "storybook": "sb-rn-get-stories --config-path .ondevice && expo start",
    "storybook:ios": "sb-rn-get-stories --config-path .ondevice && expo start --ios",
    "storybook:android": "sb-rn-get-stories --config-path .ondevice && expo start --android"
  },
  "dependencies": {
    "@expo/metro-runtime": "^2.2.16",
    "@expo/vector-icons": "^13.0.0",
    "@gorhom/bottom-sheet": "^4",
    "@react-native-async-storage/async-storage": "1.18.2",
    "@react-native-community/datetimepicker": "7.2.0",
    "@react-native-community/slider": "4.4.2",
    "@react-navigation/drawer": "^6.6.6",
    "@react-navigation/native": "^6.1.9",
    "@rneui/base": "^4.0.0-rc.8",
    "@rneui/themed": "^4.0.0-rc.8",
    "expo": "~49.0.15",
    "expo-constants": "~14.4.2",
    "expo-font": "~11.4.0",
    "expo-image": "~1.3.5",
    "expo-linear-gradient": "~12.3.0",
    "expo-linking": "~5.0.2",
    "expo-router": "^2.0.0",
    "expo-splash-screen": "~0.20.5",
    "expo-status-bar": "~1.6.0",
    "expo-system-ui": "~2.4.0",
    "expo-web-browser": "~12.3.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.72.6",
    "react-native-confirmation-code-field": "^7.3.2",
    "react-native-gesture-handler": "~2.12.0",
    "react-native-pager-view": "6.2.0",
    "react-native-popover-view": "^5.1.8",
    "react-native-reanimated": "~3.3.0",
    "react-native-safe-area-context": "4.6.3",
    "react-native-screens": "~3.22.0",
    "react-native-svg": "13.9.0",
    "react-native-switch": "^1.5.1",
    "react-native-tab-view": "^3.5.2",
    "react-native-web": "~0.19.6",
    "react-native-webview": "13.2.2",
    "styled-components": "^6.1.1",
    "styled-system": "^5.1.5",
    "usehooks-ts": "^2.9.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@storybook/addon-actions": "^6.5.16",
    "@storybook/addon-controls": "^6.5.16",
    "@storybook/addon-essentials": "^6.5.16",
    "@storybook/addon-links": "^6.5.16",
    "@storybook/addon-ondevice-actions": "^6.5.6",
    "@storybook/addon-ondevice-backgrounds": "^6.5.6",
    "@storybook/addon-ondevice-controls": "^6.5.6",
    "@storybook/addon-ondevice-notes": "^6.5.6",
    "@storybook/addon-react-native-web": "^0.0.21",
    "@storybook/builder-webpack5": "^6.5.16",
    "@storybook/manager-webpack5": "^6.5.16",
    "@storybook/react": "^6.5.16",
    "@storybook/react-native": "^6.5.6",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^12.4.1",
    "@types/jest": "^29.5.11",
    "@types/react": "~18.2.14",
    "@types/styled-system": "^5.1.22",
    "@types/url-parse": "^1.4.11",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "babel-loader": "^8.2.3",
    "babel-plugin-react-docgen-typescript": "^1.5.1",
    "babel-plugin-react-native-web": "^0.18.10",
    "cross-env": "^7.0.3",
    "eslint": "^8.53.0",
    "eslint-config-universe": "^12.0.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "husky": "^8.0.3",
    "jest": "^29.2.1",
    "jest-expo": "~49.0.0",
    "lint-staged": "^15.1.0",
    "plop": "^4.0.0",
    "prettier": "^3.1.0",
    "react-native-svg-transformer": "^1.1.0",
    "react-test-renderer": "18.2.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.1.3"
  },
  "overrides": {
    "react-refresh": "~0.14.0",
    "react-docgen-typescript": "2.2.2",
    "@storybook/react-docgen-typescript-plugin": "1.0.6--canary.9.cd77847.0"
  },
  "resolutions": {
    "react-refresh": "~0.14.0",
    "react-docgen-typescript": "2.2.2",
    "@storybook/react-docgen-typescript-plugin": "1.0.6--canary.9.cd77847.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "yarn run lint:eslint"
    ],
    "*.{js,jsx,ts,tsx,scss}": [
      "prettier --write"
    ]
  }
}
